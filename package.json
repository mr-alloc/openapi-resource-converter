    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.4",
    "commander": "^12.1.0",
    "js-yaml": "^4.1.0",
    "readline": "^1.3.0",
    "yaml": "^2.6.1"
  },
