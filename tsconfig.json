{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "paths": {
      "@/*": ["src/*"],
      "api": ["api/*"]
    },
    "sourceMap": true, //TS 콜스택
    "inlineSourceMap": false
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}
